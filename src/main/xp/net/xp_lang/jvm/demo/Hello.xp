package net.xp_lang.jvm.demo;

/**
 * Usage: Hello [name]
 */
public class Hello {

  /**
   * Entry point
   */
  public static void main(string[] $args) {
    if ($args.length > 0) {
      util.cmd.Console::writeLine('Hello ', $args[0]);
    } else {
      util.cmd.Console::writeLine(self::class.getComment());
    }
  }
}